// Each #kernel tells which function to compile; you can have many kernels
#pragma kernel CSMain

struct Food
{
    float3 position;
    int inRange;
};

RWStructuredBuffer<Food> foods;
float3 fishPosition;
float range;

[numthreads(100,1,1)]
void CSMain (uint3 id : SV_DispatchThreadID)
{
    Food food = foods[id.x];
    food.inRange = 1;
}
